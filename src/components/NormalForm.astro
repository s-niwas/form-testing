                              <div>
                                <!-- check to mark emailid field as type email, and other mandatory fields as type required -->
                                <input
                                  name="MOBILE"
                                  changeitem="SIGNUP_FORM_FIELD"
                                  maxlength="100"
                                  type="text"
                                  required="true"
                                />
                                <span style="display:none" id="dt_MOBILE"
                                  >1,true,1,Mobile,2</span
                                >
                              </div>
                            </div><div></div>
                          </div>
                        </div><!-- Captcha for Signup -->
                        <div
